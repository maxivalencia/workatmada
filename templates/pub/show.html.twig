{% extends 'base_admin.html.twig' %}

{% block title %}Pub{% endblock %}

{% block body %}
    <h1>Pub</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ pub.id }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ pub.image }}</td>
            </tr>
            <tr>
                <th>Lien</th>
                <td>{{ pub.lien }}</td>
            </tr>
            <tr>
                <th>Isactive</th>
                <td>{{ pub.isactive ? 'Oui' : 'Non' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_pub_index') }}">back to list</a>

    <a href="{{ path('app_pub_edit', {'id': pub.id}) }}">edit</a>

    {{ include('pub/_delete_form.html.twig') }}
{% endblock %}

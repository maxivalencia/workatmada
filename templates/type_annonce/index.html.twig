{% extends 'base.html.twig' %}

{% block title %}TypeAnnonce index{% endblock %}

{% block body %}
    <h1>TypeAnnonce index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Type</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_annonce in type_annonces %}
            <tr>
                <td>{{ type_annonce.id }}</td>
                <td>{{ type_annonce.type }}</td>
                <td>
                    <a href="{{ path('app_type_annonce_show', {'id': type_annonce.id}) }}">show</a>

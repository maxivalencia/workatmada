        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <div class="navigation">
        {{ knp_pagination_render(type_annonces) }}
    </div>

    <a href="{{ path('app_type_annonce_new') }}">Create new</a>
{% endblock %}

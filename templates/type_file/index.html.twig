{% extends 'base.html.twig' %}

{% block title %}TypeFile index{% endblock %}

{% block body %}
    <h1>TypeFile index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Type</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_file in type_files %}
            <tr>
                <td>{{ type_file.id }}</td>
                <td>{{ type_file.type }}</td>
                <td>
                    <a href="{{ path('app_type_file_show', {'id': type_file.id}) }}">show</a>
                    <a href="{{ path('app_type_file_edit', {'id': type_file.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_type_file_new') }}">Create new</a>
{% endblock %}
